   "source": [
    "import matplotlib.pyplot as plt\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "π = np.pi\n",
    "def area_circulo(r):\n",
    "    A = (r**2)*π\n",
    "    return A"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "l_r = np.arange(0, 10, 0.1)\n",
    "\n",
    "l_A = area_circulo(l_r)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
